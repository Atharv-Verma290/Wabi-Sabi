<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F8EDEB"
    tools:context=".quizstart">

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/floatingActionButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:backgroundTint="#F8EDEB"
        android:clickable="true"
        android:elevation="0dp"
        android:scaleType="fitCenter"
        app:borderWidth="0dp"
        app:elevation="0dp"
        app:fabSize="normal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.048"
        app:srcCompat="@drawable/quitquiz" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="301dp"
        android:layout_height="59dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="132dp"
        android:gravity="top"
        android:letterSpacing="0.04"
        android:text="@string/how_are_you"
        android:textAppearance="@style/how_are_you"
        android:textSize="23dp"
        app:layout_constraintBottom_toTopOf="@+id/option1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button5"
        android:layout_width="238dp"
        android:layout_height="56dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="92dp"
        android:layout_marginBottom="35dp"
        android:backgroundTint="#F89999"
        android:text="@string/continprof"
        android:textColor="#360D14"
        android:textSize="20sp"
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4"
        app:layout_constraintVertical_bias="0.947" />

    <View
        android:id="@+id/option1"
        android:layout_width="135dp"
        android:layout_height="129dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:background="@drawable/rectangle_3"
        android:contextClickable="true"
        app:layout_constraintBottom_toTopOf="@+id/option3"
        app:layout_constraintEnd_toStartOf="@+id/option2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/option3"
        app:layout_constraintTop_toTopOf="@+id/option2"
        app:layout_constraintVertical_bias="0.0" />

    <View
        android:id="@+id/option2"
        android:layout_width="135dp"
        android:layout_height="129dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="44dp"
        android:layout_marginBottom="44dp"
        android:background="@drawable/rectangle_3"
        android:contextClickable="true"
        android:visibility="visible"
        app:layout_constraintBottom_toTopOf="@+id/option4"
        app:layout_constraintEnd_toEndOf="parent" />

    <View
        android:id="@+id/option3"
        android:layout_width="135dp"
        android:layout_height="129dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="52dp"
        android:background="@drawable/rectangle_3"
        android:contextClickable="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/option4"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.762" />

    <View
        android:id="@+id/option4"
        android:layout_width="135dp"
        android:layout_height="129dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="44dp"
        android:background="@drawable/rectangle_3"
        android:contextClickable="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.762" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="254dp"
        android:layout_height="152dp"
        android:layout_marginTop="104dp"
        android:layout_marginBottom="316dp"
        android:foregroundGravity="center"
        app:layout_constraintBottom_toTopOf="@+id/button5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/launda" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="78dp"
        android:layout_height="29dp"
        android:gravity="center"
        android:text="@string/cheerful"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/option1"
        app:layout_constraintEnd_toEndOf="@+id/option1"
        app:layout_constraintStart_toStartOf="@+id/option1"
        app:layout_constraintTop_toTopOf="@+id/option1" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="78dp"
        android:layout_height="29dp"
        android:gravity="center"
        android:text="@string/gloomy"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/option2"
        app:layout_constraintEnd_toEndOf="@+id/option2"
        app:layout_constraintStart_toStartOf="@+id/option2"
        app:layout_constraintTop_toTopOf="@+id/option2" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="78dp"
        android:layout_height="29dp"
        android:gravity="center"
        android:text="@string/agony"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/option3"
        app:layout_constraintEnd_toEndOf="@+id/option3"
        app:layout_constraintStart_toStartOf="@+id/option3"
        app:layout_constraintTop_toTopOf="@+id/option3" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="78dp"
        android:layout_height="29dp"
        android:gravity="center"
        android:text="@string/fear"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/option4"
        app:layout_constraintEnd_toEndOf="@+id/option4"
        app:layout_constraintStart_toStartOf="@+id/option4"
        app:layout_constraintTop_toTopOf="@+id/option4" />


</androidx.constraintlayout.widget.ConstraintLayout>